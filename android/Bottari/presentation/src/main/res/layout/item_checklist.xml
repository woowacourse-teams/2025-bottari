<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/cl_checklist_item"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginHorizontal="@dimen/space_large"
    android:layout_marginTop="@dimen/space_2x_small"
    android:layout_marginBottom="@dimen/space_x_small"
    android:background="@drawable/bg_white_radius_12dp"
    android:elevation="2dp"
    android:padding="@dimen/space_median">

    <TextView
        android:id="@+id/tv_checklist_item_title"
        style="@style/Pretendard_Medium_20"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:textColor="@color/black"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:text="•  시계" />

</androidx.constraintlayout.widget.ConstraintLayout>
